{
    "name": "adriel/laravel-tinymce-simple-imageupload",
    "description": "Simple image upload for TinyMCE in Laravel.",
    "version": "1.3.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel 5",
        "tinymce",
        "image upload",
        "php"
    ],
    "homepage": "https://github.com/Adrielpin/laravel-tinymce-simple-imageupload",
    "authors": [
        {
            "name": "Adriel Pinheiro",
            "email": "adriel@pinheiro.xyz",
            "homepage": "https://pinheiro.xyz"
        }
    ],
    "require": {
        "illuminate/support": "~5.1|~8.0",
        "php" : "~5.5|~7.3"
    },
    "autoload": {
        "psr-4": {
            "Petehouston\\Tinymce\\": "src"
        }
    },
    "extra": {
        "laravel": {
